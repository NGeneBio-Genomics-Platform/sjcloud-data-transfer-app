{
  "name": "SJCloudUploader",
  "version": "1.0.0a",
  "description": "The St. Jude Cloud desktop app, designed for getting data in the St. Jude Cloud easily and reliably.",
  "homepage": "https://github.com/claymcleod/sjcloud-data-transfer",
  "main": "app/main.js",
  "scripts": {
    "start": "electron app/main.js",
    "www": "webpack --config=webpack.config.js --watch --progress --colors",
    "www-no-watch": "webpack --config=webpack.config.js --progress --colors",
    "postinstall": "install-app-deps",
    "pack": "build -m -l -w --dir",
    "dist:mac": "build -m",
    "dist:linux": "build -l",
    "dist:win": "build -w",
    "dist:all": "build -lwm --x64 --ia32",
    "release:prerelease": "build -lwm --x64 --ia32 --prerelease -p",
    "release:all": "build -lwm --x64 --ia32 -p",
    "postdist:win": "build\\scripts\\change_manifest.bat",
    "test": "karma start karma.config.js"
  },
  "author": {
	  "name": "Clay McLeod, Andrew Frantz, Edgar Sioson",
	  "email": "clay.mcleod@stjude.org"
  },
  "build": {
    "appId": "cloud.stjude",
	"productName": "SJCP Data Transfer",
    "dmg": {},
    "linux": {
	  "category": "Utility",
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "async": "^2.4.1",
    "bootstrap": "^3.3.7",
    "expand-home-dir": "0.0.3",
    "express": "^4.15.4",
    "glob-to-regexp": "^0.3.0",
    "jquery": "^3.2.1",
    "mkdirp": "^0.5.1",
    "pem": "^1.9.7",
    "spinkit": "^1.2.5",
    "tippy.js": "^1.2.0",
    "todc-bootstrap": "^3.3.7-3.3.13",
    "tree-kill": "^1.1.0",
    "vue-tippy": "^0.2.8",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "css-loader": "^0.28.4",
    "d3-selection": "^1.1.0",
    "electron": "~1.6.2",
    "electron-builder": "^19.8.0",
    "electron-packager": "latest",
    "eslint": "^4.0.0",
    "eslint-config-google": "^0.8.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "jasmine": "^2.8.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-webpack": "^2.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "null-loader": "^0.1.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.2",
    "vue2-dropzone": "^2.3.5",
    "vuex": "^2.3.1",
    "webpack": "^3.5.3",
    "webpack-notifier": "^1.5.0"
  }
}
