{
  "name": "SJCPUploader",
  "version": "1.0.0",
  "description": "The St. Jude Cloud desktop app, designed for getting data in the St. Jude Cloud easily and reliably.",
  "main": "app/main.js",
  "scripts": {
    "start": "electron app/main.js",
    "www": "webpack --config=webpack.config.js --watch --progress --colors",
    "www-no-watch": "webpack --config=webpack.config.js --progress --colors",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist:mac": "build -m",
    "dist:linux": "build -l",
    "dist:win": "build -w",
    "postdist:win": "scripts\\change_manifest.bat"
  },
  "author": "Clay McLeod, Andrew Frantz, Edgar Sioson",
  "build": {
    "appId": "org.stjude.cloud",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "async": "^2.4.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^3.3.7",
    "expand-home-dir": "0.0.3",
    "express": "^4.15.4",
    "jquery": "^3.2.1",
    "mkdirp": "^0.5.1",
    "pem": "^1.9.7",
    "spinkit": "^1.2.5",
    "tippy.js": "^1.2.0",
    "todc-bootstrap": "^3.3.7-3.3.13",
    "tree-kill": "^1.1.0",
    "vue-tippy": "^0.2.8",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "css-loader": "^0.28.4",
    "electron": "~1.6.2",
    "electron-builder": "^19.8.0",
    "electron-packager": "latest",
    "eslint": "^4.0.0",
    "eslint-config-google": "^0.8.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "null-loader": "^0.1.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue": "^2.4.2",
    "vue-loader": "^13.0.4",
    "vue-router": "^2.7.0",
    "vue-template-compiler": "^2.4.2",
    "vue2-dropzone": "^2.3.5",
    "vuex": "^2.3.1",
    "webpack": "^3.5.3",
    "webpack-notifier": "^1.5.0"
  }
}
